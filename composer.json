{
    "name": "capivaralearn/app",
    "description": "CapivaraLearn Application",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "illuminate/database": "^12.20",
        "monolog/monolog": "^3.9",
        "php-di/php-di": "^7.0",
        "phpmailer/phpmailer": "^6.8",
        "slim/http": "^0.4.0",
        "slim/psr7": "^1.7",
        "slim/slim": "4.*",
        "twig/twig": "^3.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "CapivaraLearn\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
